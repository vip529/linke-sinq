{
  "name": "linke-sinq",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "pnpm run check",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ui:open": "vitest --ui --open",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "type-check": "tsc --noEmit",
    "circular": "madge --circular --extensions ts,tsx src/",
    "circular:graph": "madge --circular --extensions ts,tsx --image graph.svg src/",
    "generate:theme": "ts-node --project tsconfig.scripts.json scripts/generate-theme-css.ts",
    "check": "pnpm run generate:theme && pnpm run lint && pnpm run type-check && pnpm run test:run && pnpm run circular"
  },
  "dependencies": {
    "@base-ui-components/react": "1.0.0-beta.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "next": "^16.0.3",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "jsdom": "^27.2.0",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "sass": "^1.94.0",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.8"
  }
}
